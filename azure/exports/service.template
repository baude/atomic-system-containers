[Unit]
Description=WALA Agent
Wants=network-online.target sshd.service sshd-keygen.service
After=network-online.target

[Service]
Environment=PYTHONPATH=/var/lib/containers/atomic/wala-container.0/rootfs/usr/lib/python2.7/site-packages/
ExecStart=/usr/bin/python  -u usr/sbin/waagent -daemon
Restart=always
RestartSec=5
WorkingDirectory=/var/lib/containers/atomic/wala-container.0/rootfs
RuntimeDirectory=wala-container

[Install]
WantedBy=multi-user.target
